{
  "name": "@easy-kmu/web",
  "description": "Manage quotes, invoices, and clients with ease.",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "homepage": "./",
  "scripts": {
    "dev": "yarn start",
    "start": "TZ=UTC concurrently --prefix name --prefix-colors \"yellow,magenta\" --names \"üîôüîö,üï∏Ô∏è\" \"yarn run dev:server\" \"yarn run dev:client\"",
    "dev:server": "yarn tsup --tsconfig tsconfig.server.json --watch --onSuccess \"node build-server/index.mjs\" src/server/index.ts",
    "dev:client": "wait-on -l https://ekmu.localhost:8080 && PORT=3000 vite",
    "monitor:redis": "redis-cli monitor",
    "build": "NODE_ENV=production INLINE_RUNTIME_CHUNK=false yarn run build-raw",
    "build-raw": "yarn run build:client && yarn run build:server",
    "build:server": "yarn tsc:server && yarn tsup --tsconfig tsconfig.server.json src/server/index.ts",
    "build:client": "tsc && vite build",
    "build:client:preview": "vite preview",
    "test": "TZ=UTC vitest",
    "lint": "biome lint --apply ./src && tsc --noEmit",
    "lint:unsafe": "biome lint --write  --unsafe ./src && tsc --noEmit",
    "lint:check": "biome lint ./src",
    "format": "biome format --write ./src",
    "format:check": "biome format --check ./src",
    "tsc:client:watch": "tsc --watch --noEmit",
    "tsc:server": "tsc --noEmit --project tsconfig.server.json",
    "tsc:server:watch": "tsc --watch --noEmit --project tsconfig.server.json",
    "cert": "mkdir -p certs && mkcert -key-file certs/server.key -cert-file certs/server.pem ekmu.localhost localhost 127.0.0.1",
    "docker:build": "yarn buildinfo:update && docker build -t worklog . && docker tag worklog nightfury5432/worklog",
    "docker:push": "docker push nightfury5432/worklog",
    "buildinfo:dummy": "cp -f src/buildInfo.ts.dummy src/buildInfo.ts",
    "buildinfo:update": "yarn run src/scripts/updateBuildInfo.ts",
    "certs:all": "yarn run certs:server",
    "certs:server": "mkdir -p certs && mkcert -key-file certs/server-key.pem -cert-file certs/server-cert.pem ekmu.localhost localhost",
    "scripts:importTestdata": "yarn run tsx src/scripts/importTestdata.ts"
  },
