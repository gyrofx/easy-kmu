{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "yarn",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev:server"],
      "cwd": "${workspaceRoot}",
      "timeout": 10000
    },
    {
      "type": "node",
      "request": "launch",
      "name": "node server",
      "program": "${workspaceFolder}/src/server/index.ts",
      "sourceMaps": true,
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
